        <externalId>false</externalId>
        <formula>IF(
AND(NOT(ISBLANK(Job_Start_Date__c)), ISBLANK(Job_End_Date__c)), TODAY() - Job_Start_Date__c,
IF(
AND(NOT(ISBLANK(Job_Start_Date__c)), NOT(ISBLANK(Job_End_Date__c))),  Job_End_Date__c - Job_Start_Date__c, 0
)
)</formula>
