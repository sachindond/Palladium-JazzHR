        <formula>IF(
AND(OR(Applicant_Progress__c = &quot;Lambeth Candidate&quot;,Applicant_Progress__c = &quot;Wandsworth Candidate&quot;,Applicant_Progress__c = &quot;Full Time&quot;,Applicant_Progress__c = &quot;Part Time&quot;,Applicant_Progress__c = &quot;Found Employment Elsewhere&quot;), NOT(ISBLANK(Job_Start_Date__c))), &quot;Job Start&quot;,
IF(
AND(OR(Applicant_Progress__c = &quot;Lambeth Candidate&quot;,Applicant_Progress__c = &quot;Wandsworth Candidate&quot;,Applicant_Progress__c = &quot;Full Time&quot;,Applicant_Progress__c = &quot;Part Time&quot;,Applicant_Progress__c = &quot;Found Employment Elsewhere&quot;), ISBLANK(Job_Start_Date__c)), &quot;Job Offer&quot;, Applicant_Progress__c
